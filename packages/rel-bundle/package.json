{
  "name": "rel-bundle",
  "version": "0.0.2",
  "description": "Visit https://rel.run/docs to view the full documentation.",
  "author": "Ian Hunter <ian@rel.run>",
  "license": "ISC",
  "main": "dist/index.mjs",
  "type": "module",
  "exports": {
    "client": "./dist/client.js",
    "server": "./dist/server.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "tsup": {
    "entry": [
      "src"
    ],
    "format": [
      "esm"
    ],
    "legacy-output": true,
    "splitting": false,
    "sourcemap": true,
    "dts": true,
    "clean": true
  },
  "dependencies": {
    "js-cookie": "^3.0.1",
    "rel-client": "workspace:^0.0.0",
    "rel-server": "workspace:^0.1.0"
  },
  "devDependencies": {
    "ipjs": "^5.2.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
